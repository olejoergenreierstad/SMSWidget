# Widget origin (for debug host simulator)
VITE_WIDGET_ORIGIN=http://localhost:5173

# Default tenant/install when not in URL
VITE_DEFAULT_TENANT=demo
VITE_DEFAULT_INSTALL=default

# Host API base URL (optional - can be passed via HOST_ACK)
VITE_HOST_API_DEFAULT=

# Firebase config (required for Firestore theme loading)
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=

# API base for Cloud Functions (e.g. https://europe-north1-PROJECT.cloudfunctions.net)
VITE_API_BASE=

# Admin secret for setupTenant (beskytt opprettelse av kunder)
VITE_SETUP_ADMIN_SECRET=

# SMS providers (Cloud Functions â€“ sett via firebase functions:config eller Secret Manager)
# Sveve (Norge/Europa):
# SMS_SVEVE_USER=
# SMS_SVEVE_PASSWD=
# SMS_SVEVE_FROM=  (valgfri avsendernavn)
# Twilio (global / USA / UK):
# SMS_TWILIO_ACCOUNT_SID=
